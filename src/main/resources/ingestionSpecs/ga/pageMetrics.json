{
  "type": "index_parallel",
  "spec": {
    "ioConfig": {
      "type": "index_parallel",
      "inputSource": {
        "type": "s3",
        "uris": [
          "s3://engati-druid-dev/data/{{customer_id}}/{{bot_ref}}/{{load_path}}/PAGE_METRICS.csv"
        ],
        "properties": {
          "accessKeyId": {
            "type": "default",
            "password": "{{access_key}}"
          },
          "secretAccessKey": {
            "type": "default",
            "password": "{{secret_key}}"
          }
        }
      },
      "inputFormat": {
        "type": "csv",
        "findColumnsFromHeader": true
      },
      "appendToExisting": true
    },
    "tuningConfig": {
      "type": "index_parallel",
      "partitionsSpec": {
        "type": "dynamic"
      },
      "logParseExceptions": true
    },
    "dataSchema": {
      "dataSource": "page_metrics_{{customer_id}}_{{bot_ref}}",
      "granularitySpec": {
        "type": "uniform",
        "queryGranularity": "DAY",
        "rollup": false,
        "segmentGranularity": "WEEK"
      },
      "timestampSpec": {
        "column": "date",
        "format": "auto"
      },
      "dimensionsSpec": {
        "dimensions": [
          {
            "type":"string",
            "name":"page_path"
          },
          {
            "type":"string",
            "name":"landing_page_path"
          },
          {
            "type":"double",
            "name":"bounce_rate"
          },
          {
            "type":"long",
            "name":"page_views"
          },
          {
            "type": "long",
            "name": "unique_page_views"
          },
          {
            "type":"long",
            "name":"time_on_page"
          },
          {
            "type":"long",
            "name":"page_load_time"
          }
        ]
      }
    }
  }
}