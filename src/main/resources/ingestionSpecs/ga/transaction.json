{
  "type": "index_parallel",
  "spec": {
    "ioConfig": {
      "type": "index_parallel",
      "inputSource": {
        "type": "s3",
        "uris": [
          "s3://engati-druid-dev/data/{{customer_id}}/{{bot_ref}}/{{load_path}}/TRANSACTION.csv"
        ],
        "properties": {
          "accessKeyId": {
            "type": "default",
            "password": "{{access_key}}"
          },
          "secretAccessKey": {
            "type": "default",
            "password": "{{secret_key}}"
          }
        }
      },
      "inputFormat": {
        "type": "csv",
        "findColumnsFromHeader": true
      },
      "appendToExisting": true
    },
    "tuningConfig": {
      "type": "index_parallel",
      "partitionsSpec": {
        "type": "dynamic"
      },
      "logParseExceptions": true
    },
    "dataSchema": {
      "dataSource": "transaction_{{customer_id}}_{{bot_ref}}",
      "granularitySpec": {
        "type": "uniform",
        "queryGranularity": "DAY",
        "rollup": false,
        "segmentGranularity": "WEEK"
      },
      "timestampSpec": {
        "column": "date",
        "format": "auto"
      },
      "dimensionsSpec": {
        "dimensions": [
          {
            "type":"string",
            "name":"transaction_id"
          },
          {
            "type":"string",
            "name":"source"
          },
          {
            "type":"string",
            "name":"medium"
          },
          {
            "type":"string",
            "name":"device_category"
          },
          {
            "type":"string",
            "name":"campaign"
          },
          {
            "type":"string",
            "name":"browser"
          },
          {
            "type": "double",
            "name": "revenue"
          },
          {
            "type": "double",
            "name": "aov"
          }
        ]
      }
    }
  }
}